{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "dynamicImport": true,
      "decorators": true,
      "dts": true
    },
    "target": "es2020",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false
  },
  "minify": false,
  "module": {
    "type": "commonjs",
    "noInterop": false,
    "lazy": false,
    "strict": true,
    "ignoreDynamic": true
  }
}
